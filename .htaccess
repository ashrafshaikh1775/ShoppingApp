RewriteEngine On
#RewriteBase /ShoppingApp/

RewriteCond %{THE_REQUEST} /([^.]*)\.([A-zA-Z]) [NC]
RewriteRule (.*) /%1 [R=302,L]  


#RewriteCond %{REQUEST_FILENAME} !-f
